{
  "plugins": ["jazer","quiz","pywebtest-gitbook"],
  "pluginsConfig": {
      "brythonPath": "https://raw.githubusercontent.com/saturngod/gitbook-plugin-pyweb/master/book/js/brython.js",
      "jazer": {
          "editorAutoHeight": "solution"
      },
    "quiz": {
            "labels": {
                "check"             : "Antwort kontrollieren",
                "showExplanation"   : "Hinweis", 
                "showCorrect"       : "korrekte Antwort",   
                "explanationHeader" : "Hinweis"
            },
            "text": {
                "noChosen"    : "Choose at least one answer",  
                "incomplete"  : "Some correct answers are missing" 
            },
            "buttons": {
                "showCorrect"       : true, 
                "showExplanation"   : true 
            }
    }
  }
}
